<% for(var i = 0;i<myFirstDatabase.length;i++){%>
    <tr>
        <td>
            <%= i+1 %>
        </td>
        <td>
            <%= myFirstDatabase[i].BandName%>
        </td>
        <td>
            <%= myFirstDatabase[i].Genre%>
        </td>
        <td>
            <%= myFirstDatabase[i].Country%>
        </td>
        <td>
            <%= myFirstDatabase[i].Status%>
        </td>
        <td>
            <a href="/update-user?id=<%=myFirstDatabase[i]._id%>" class="btn border-shadow update"><span class="text-gradient"><i class="fas fa-edit"></i></span></a><a href="/" class="btn border-shadow delete" data-id=<%=myFirstDatabase[i]._id%>><span class="text-gradient"><i class="fas fa-trash-alt"></i></span></a>
        </td>
    </tr>
    <% } %>